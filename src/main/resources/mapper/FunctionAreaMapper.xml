<mapper namespace="com.qiangdong.reader.dao.FunctionAreaMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qiangdong.reader.entity.FunctionArea">
        <id column="id" property="id" />
